		  ");",
		  "",
		  "// Adds superior menu.",
		  "add_action( 'genesis_before_header', 'cmk_topbar' );",
		  "function cmk_topbar() {",
		  "",
		  "\tif ( is_page_template( 'page_landing.php' ) ) {",
		  "\t\treturn;",
		  "\t}",
		  "\t?>",
		  "\t<div class=\"topbar\">",
		  "\t\t<div class=\"wrap\">",
		  "\t\t\t\t<div class=\"topbar-left one-half first\">",
		  "\t\t\t\t\t<?php dynamic_sidebar( 'topbar-left' ); ?>",
		  "\t\t\t\t</div>",
		  "\t\t\t\t<div class=\"topbar-right one-half\">",
