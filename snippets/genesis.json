	"Genesis | Adds HTML elements to main menu": {
		"prefix": "gensmen",
		"body": [
			"//* Adds custom html elements to the main menu",
			"add_filter( 'wp_nav_menu_items', 'cmk_menu_add_element', 10, 2 );",
			"function cmk_menu_add_element ( \\$items, \\$args ) {",
			"\tif (\\$args->theme_location == 'primary') {",
			"\t\t\\$menu_html = '';",
			"\t\t\\$items = \\$items.\\$menu_html;",
			"\t}",
			"\treturn \\$items;",
			"}"
		]
	},
