	"Genesis | Adds a Topbar in site": {
		"prefix": "genstopb",
		"body": [
		  "",
		  "// * In functions",
		  "/**",
		  " * # Topbar with two sidebars",
		  " * ---------------------------------------------------------------------------------------------------- */",
		  "",
		  "// Register new sidebar topbar-left.",
		  "genesis_register_sidebar(",
		  "\tarray(",
		  "\t\t'id'   => 'topbar-left',",
		  "\t\t'name' => 'Barra Superior Izquierda',",
		  "\t)",
		  ");",
		  "",
		  "// Register new sidebar topbar-left.",
		  "genesis_register_sidebar(",
		  "\tarray(",
		  "\t\t'id'   => 'topbar-right',",
		  "\t\t'name' => 'Barra Superior Derecha',",
